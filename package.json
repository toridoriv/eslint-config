{
  "name": "@toridoriv/eslint-config",
  "version": "0.1.0",
  "type": "module",
  "description": "A custom Eslint configuration for my personal projects.",
  "main": "index.js",
  "typings": "typings.d.ts",
  "bin": {
    "setup-eslint-config": "./bin/setup-eslint-config"
  },
  "files": [
    "bin",
    "lib",
    "index.js",
    "typings.d.ts"
  ],
  "author": {
    "name": "Victoria Rodriguez",
    "email": "vrodriguezfe@icloud.com",
    "url": "https://github.com/toridoriv"
  },
  "directories": {
    "bin": "./bin"
  },
  "license": "GPLv3",
  "homepage": "https://github.com/toridoriv/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toridoriv/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/toridoriv/eslint-config/issues",
    "email": "vrodriguezfe@icloud.com"
  },
  "devDependencies": {
    "eslint": "^9.22.0",
    "type-fest": "^4.37.0"
  },
  "scripts": {},
  "dependencies": {
    "@html-eslint/parser": "^0.35.2",
    "@toridoriv/prettier-plugin-jsdoc": "github:toridoriv/prettier-plugin-jsdoc",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.6",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "jsonc-simple-parser": "^3.0.0",
    "markdown-eslint-parser": "^1.2.1",
    "prettier": "3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "typescript": "^5.8.2"
  },
  "config": {
    "@toridoriv": {
      "files": {
        "ignore": {
          "generated": {
            "title": "Automatically generated",
            "paths": [
              "DS_Store",
              "node_modules",
              "package-lock.json",
              "coverage"
            ]
          },
          "exceptions": {
            "title": "Do not ignore",
            "paths": [
              "!bin/**",
              "!.vscode/**"
            ]
          },
          "logs": {
            "title": "Logs",
            "paths": [
              "logs",
              "*.log",
              "npm-debug.log*"
            ]
          },
          "temporal": {
            "title": "Temporary",
            "paths": [
              "tmp/",
              "*.tmp",
              "tmp.*",
              "var/tmp"
            ]
          }
        }
      }
    }
  }
}
